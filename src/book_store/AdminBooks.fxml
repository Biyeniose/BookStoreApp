<AnchorPane id="AnchorPane" prefHeight="430.0" prefWidth="650.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="book_store.AdminBooksController">
   <children>
      <Label alignment="CENTER" layoutX="22.0" layoutY="14.0" prefHeight="30.0" prefWidth="606.0" text="ADMIN Books Page">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <TableView fx:id="table" layoutX="69.0" layoutY="63.0" prefHeight="234.0" prefWidth="478.0">
        <columns>
          <TableColumn fx:id="bookname" prefWidth="306.0" text="BookName" />
          <TableColumn fx:id="price" prefWidth="171.0" text="Price" />
        </columns>
      </TableView>
      <Button fx:id="backButton" layoutX="133.0" layoutY="381.0" mnemonicParsing="false" onAction="#clickBack" prefHeight="27.0" prefWidth="72.0" text="Back">
