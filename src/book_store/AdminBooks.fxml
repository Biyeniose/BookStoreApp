            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="delete" layoutX="426.0" layoutY="381.0" mnemonicParsing="false" onAction="#clickDelete" prefHeight="27.0" prefWidth="72.0" text="Delete">
